<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tuturu_test_api21">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">



            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectStationActivity"
            android:label="@string/selectStationActivityLabel" />
        <activity
            android:name=".StationInfoActivity"
            android:label="@string/stationInformationActivityLabel" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/aboutActivityLabel" />
        <activity
            android:name=".CalendarActivity"
            android:label="@string/calendarActivityLabel"
            >

        </activity>
    </application>

</manifest>